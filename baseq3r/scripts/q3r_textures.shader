	{
		map textures/q3r_building/wirehead.tga
		blendfunc add
		rgbGen wave sin 2 1.2 0 4 
	}
	{
		map textures/base_wall/comp3text.tga
		blendfunc add
		rgbGen wave square 0.5 0.5 0 8 
		tcMod scroll 0 -2
	}
}

